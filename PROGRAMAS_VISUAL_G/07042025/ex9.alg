algoritmo "Nome do Programa"

var
    a,b,c: inteiro


inicio
    escreva("Digite um valor: ")
    leia(a)
    escreva("Digite outro valor: ")
    leia(b)
    c<-a%b
    se (c=0) entao
        escreval (a," é divisivel por ",b)
    senao
        escreval (a,"nao é divisivel por ",b)
    fimse

fimalgoritmo