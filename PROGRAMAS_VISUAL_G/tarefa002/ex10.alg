algoritmo "Nome do Programa"

var
    a,b,c: inteiro


inicio
    escreva("Digite um valor: ")
    leia(a)
    escreva("Digite outro valor: ")
    leia(b)
    se (a>b) entao
        escreval (a," é maior que ",b)
    se (a<b) entao
        escreval (a,"nao pe maior que ",b)
    fimse
    senao
        escreva("os valores são iguais ")
    fimse

fimalgoritmo