algoritmo "Nome do Programa"

var
    a:inteiro


inicio
    escreva("Digite um valor: ")
    leia(a)
    escolha(a)
        caso 20
            escreva("a=20")
        outrocaso
            se a > 20 entao
                escreva("a>20")
            senao
                escreva("a<20")
            fimse
    fimescolha



fimalgoritmo